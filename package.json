{
  "name": "swiss-poliytics",
  "version": "1.0.0",
  "description": "Angular and Bootstrap 4 Admin Template.",
  "author": "info@adriankrebs.ch",
  "homepage": "tbd",
  "repository": {
    "type": "git",
    "url": "https://github.com/AdrianKrebs/swiss-poliytics"
  },
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:hmr": "ng serve --hmr",
    "start:aot": "ng serve --aot",
    "build": "rimraf dist && ng build",
    "build:prod": "npm run build -- --prod",
    "build:prod:aot": "npm run build:prod -- --aot",
    "build:ci": "npm-run-all -p -r build:prod build:prod:aot",
    "test": "ng test -sr",
    "test:coverage": "rimraf coverage && npm run test -- -cc",
    "lint": "ng lint --type-check",
    "lint:fix": "ng lint --fix",
    "lint:styles": "stylelint ./src/**/*.scss",
    "lint:ci": "npm run lint && npm run lint:styles",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "ng e2e",
    "prepush": "npm run lint:ci"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "4.3.2",
    "@angular/common": "4.3.2",
    "@angular/compiler": "4.3.2",
    "@angular/core": "4.3.2",
    "@angular/forms": "4.3.2",
    "@angular/http": "4.3.2",
    "@angular/platform-browser": "4.3.2",
    "@angular/platform-browser-dynamic": "4.3.2",
    "@angular/platform-server": "4.3.2",
    "@angular/router": "4.3.2",
    "@ng-bootstrap/ng-bootstrap": "1.0.0-beta.5",
    "@ngx-translate/core": "6.0.1",
    "@ngx-translate/http-loader": "0.0.3",
    "@types/ramda": "^0.24.12",
    "amcharts3": "github:amcharts/amcharts3",
    "animate.css": "3.5.2",
    "chart.js": "1.1.1",
    "chroma-js": "1.3.3",
    "core-js": "2.4.1",
    "easy-pie-chart": "2.1.7",
    "font-awesome": "4.7.0",
    "ionicons": "2.0.1",
    "jquery-slimscroll": "1.3.8",
    "moment": "^2.18.1",
    "ng2-completer": "^1.3.1",
    "normalize.css": "6. 0.0",
    "popper.js": "^1.12.5",
    "ramda": "^0.24.1",
    "resolve-url-loader": "^2.1.0",
